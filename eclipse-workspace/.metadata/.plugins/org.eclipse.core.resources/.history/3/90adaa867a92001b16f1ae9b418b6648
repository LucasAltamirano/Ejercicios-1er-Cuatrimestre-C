#include <stdio.h>
#include <stdlib.h>

int main(void) {

	setbuf(stdout,NULL);

	char loguinUser;


	printf("Debe loguearse , para empezar a operar");
	printf("\nIngrese un nombre de Usuario(maximo 10 caracteres)");
	fflush(stdin);
	scanf("%c",loguinUser);

	printf("%s",loguinUser);










	return EXIT_SUCCESS;
}
